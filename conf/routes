# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                  controllers.Assets.versioned(path="/public", file: Asset)

GET        /characters/list               siliconValley.controllers.character.CharacterController.showCharacterList
GET        /characters/api                siliconValley.controllers.api.CharacterApiController.charactersApi

GET        /charactersProfile/list        siliconValley.controllers.character.CharacterController.showCharacterProfile